<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="2 Column Template."
    jcr:primaryType="cq:Template"
    jcr:title="Content Page 2 Column"
    allowedPaths="[/content/whitbread-assignment-site(.*)$]"
    ranking="{Long}100">
    <jcr:content
        cq:designPath="/etc/designs/whitbread-assignment-site"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="whitbread-assignment-site/components/structure/layouts/page-2-columns">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="whitbread-assignment-site/components/structure/elements/header"/>
        <parsys1
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys"/>
        <parsys2
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys"/>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="whitbread-assignment-site/components/structure/elements/footer"/>
    </jcr:content>
</jcr:root>
